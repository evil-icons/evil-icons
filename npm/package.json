{
  "name": "evil-icons",
  "version": "1.7.7",
  "description": "Evil Icons is a set of SVG icons designed extensively for using in modern web projects",
  "main": "es5/index.js",
  "scripts": {
    "pretest": "jscs index.js js test && npm run linkassets",
    "test": "mocha --require babel/register",
    "posttest": "rm -rf assets",
    "transpile": "babel index.js js/*.js -d es5/",
    "prepublish": "npm run transpile && npm run copyassets",
    "postpublish": "rm -rf es5/ assets/",
    "copyassets": "rm -rf assets && cp -R ../assets .",
    "linkassets": "rm -rf assets && ln -s ../assets"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/outpunk/evil-icons.git"
  },
  "keywords": [
    "icons",
    "svg",
    "sprite"
  ],
  "author": "Alexander Madyankin <alexander@madyankin.name>",
  "contributors": [
    {
      "name": "Alexander Madyankin"
    },
    {
      "name": "Roman Shamin"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/outpunk/evil-icons/issues"
  },
  "homepage": "http://evil-icons.io",
  "devDependencies": {
    "babel": "^5.6.23",
    "jscs": "^1.13.1",
    "mocha": "^2.2.4"
  },
  "dependencies": {
    "libxmljs": "^0.14.0",
    "svgo": "^0.5.1"
  }
}
